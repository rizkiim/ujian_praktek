<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
		 
		 <changeSet id="1" author="rizki">
        <createTable tableName="tabel-liquibase">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
			<column name="nisn" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="nama" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="jurusan" type="varchar(50)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
			<column name="kelas" type="varchar(20)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
			<column name="jenkel" type="varchar(20)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
			<column name="alamat" type="varchar(20)">
				<constraints nullable="true"></constraints>
			</column>
		</createTable>
    </changeSet>

</databaseChangeLog>